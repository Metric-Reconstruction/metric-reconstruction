         SuperscriptBox[
          StyleBox["g", "TI"], "\[Mu]\[Nu]"], 
         SubsuperscriptBox[
          StyleBox["h", "TI"], "\[Mu]\[Nu]", "IRG"], "\[LongEqual]", "0", 
         ",  ", "and", "  ", 
         SuperscriptBox[
          StyleBox["n", "TI"], "\[Mu]"], 
