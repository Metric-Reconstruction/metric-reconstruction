Cell[TextData[{
 "Each ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       SubsuperscriptBox[
        StyleBox["h", "TI"], 
        RowBox[{
          StyleBox["a", "TI"], 
          StyleBox["b", "TI"]}], 
        RowBox[{"(", "\[PlusMinus]", ")"}]], TraditionalForm], "errors" -> {},
      "input" -> "h_{ab}^{(\\pm)}", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "33f50405-228e-45c8-a40d-4fca23db413c"],
 " piece of the tetrad components can be further split into ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       StyleBox["A", "TI"], TraditionalForm], "errors" -> {}, "input" -> "A", 
     "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "f2883340-0505-43e3-a6e1-649e2deb06d2"],
 " and ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       StyleBox["B", "TI"], TraditionalForm], "errors" -> {}, "input" -> "B", 
     "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "f9d6e35a-17f7-45b0-a837-b299f77e9866"],
 " pieces: ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SubsuperscriptBox[
          StyleBox["h", "TI"], 
          RowBox[{
            StyleBox["a", "TI"], 
            StyleBox["b", "TI"]}], 
          RowBox[{"(", "+", ")"}]], "\[LongEqual]", 
         StyleBox["A", "TI"], 
         SubsuperscriptBox[
          StyleBox["h", "TI"], 
          RowBox[{
            StyleBox["a", "TI"], 
            StyleBox["b", "TI"]}], 
          RowBox[{
            RowBox[{"(", "+", ")"}], 
            StyleBox["A", "TI"]}]], "+", 
         OverscriptBox[
          StyleBox["B", "TI"], "_"], 
         SubsuperscriptBox[
          StyleBox["h", "TI"], 
          RowBox[{
            StyleBox["a", "TI"], 
            StyleBox["b", "TI"]}], 
          RowBox[{
            RowBox[{"(", "+", ")"}], 
            StyleBox["B", "TI"]}]], ",  ", 
         SubsuperscriptBox[
          StyleBox["h", "TI"], 
          RowBox[{
            StyleBox["a", "TI"], 
            StyleBox["b", "TI"]}], 
          RowBox[{"(", "-", ")"}]], "\[LongEqual]", 
         OverscriptBox[
          StyleBox["A", "TI"], "_"], 
         SubsuperscriptBox[
          StyleBox["h", "TI"], 
          RowBox[{
            StyleBox["a", "TI"], 
            StyleBox["b", "TI"]}], 
          RowBox[{
            RowBox[{"(", "+", ")"}], 
            StyleBox["A", "TI"]}]], "+", 
         StyleBox["B", "TI"], 
         SubsuperscriptBox[
          StyleBox["h", "TI"], 
          RowBox[{
            StyleBox["a", "TI"], 
            StyleBox["b", "TI"]}], 
          RowBox[{
            RowBox[{"(", "-", ")"}], 
            StyleBox["B", "TI"]}]], ","}], TraditionalForm], "errors" -> {}, 
     "input" -> 
     "h_{ab}^{(+)} = A h_{ab}^{(+)A}+\\overline{B} h_{ab}^{(+)B}, \\quad\n    \
h_{ab}^{(-)}=\\overline{A}h_{ab}^{(+)A}+Bh_{ab}^{(-)B},", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "69137a86-96d9-4bab-80b1-6177ed6ca236"],
 " where ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       StyleBox["A", "TI"], TraditionalForm], "errors" -> {}, "input" -> "A", 
     "state" -> "Boxes"|>,
