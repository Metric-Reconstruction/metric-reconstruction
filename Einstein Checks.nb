Cell[BoxData[{
 RowBox[{
  RowBox[{"eqR2", "=", 
   RowBox[{
    RowBox[{
     RowBox[{
      SuperscriptBox["\[CapitalDelta]", 
       RowBox[{"-", "s"}]], "*", 
      RowBox[{"D", "[", 
       RowBox[{
        RowBox[{
         SuperscriptBox["\[CapitalDelta]", 
          RowBox[{"s", "+", "1"}]], "*", 
         RowBox[{"D", "[", 
          RowBox[{
           RowBox[{"R2", "[", 
            RowBox[{"\[Omega]", ",", "l", ",", "m", ",", "r"}], "]"}], ",", 
           "r"}], "]"}]}], ",", "r"}], "]"}]}], "+", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        FractionBox[
         RowBox[{
          SuperscriptBox[
           RowBox[{"K", "[", 
            RowBox[{"\[Omega]", ",", "m"}], "]"}], "2"], "-", 
          RowBox[{"2", "*", "I", "*", "s", "*", 
           RowBox[{"(", 
            RowBox[{"r", "-", "M"}], ")"}], "*", 
           RowBox[{"K", "[", 
            RowBox[{"\[Omega]", ",", "m"}], "]"}]}]}], "\[CapitalDelta]"], 
