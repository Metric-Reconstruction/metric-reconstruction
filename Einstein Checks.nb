Cell[TextData[{
 "We first express a generic metric perturbation in Boyer-Lindquist \
coordinates in terms of its tetrad components (",
 Cell[BoxData[
  RowBox[{
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SubscriptBox[
          StyleBox["h", "TI"], "\[Mu]\[Nu]"], "\[LongEqual]", 
         SubsuperscriptBox[
          StyleBox["e", "TI"], "\[Mu]", 
          StyleBox["a", "TI"]], 
         SubsuperscriptBox[
          StyleBox["e", "TI"], "\[Nu]", 
          StyleBox["b", "TI"]], 
         SubscriptBox[
          StyleBox["h", "TI"], 
          RowBox[{
            StyleBox["a", "TI"], 
            StyleBox["b", "TI"]}]]}], TraditionalForm], "errors" -> {}, 
     "input" -> "h_{\\mu \\nu}=e^a_\\mu e^b_\\nu h_{ab}", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], ")"}]], "Input",ExpressionUUID->
  "d45c2b8d-9b86-4665-87a6-34b58c1b3b51"],
