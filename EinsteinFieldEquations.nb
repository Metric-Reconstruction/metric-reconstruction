         RowBox[{"t", ",", "r", ",", "\[Theta]", ",", "\[Phi]"}], "}"}], ",", 
        "0"}], "]"}]}], ",", 
     RowBox[{"hlmf", "\[Rule]", 
      RowBox[{"Function", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"t", ",", "r", ",", "\[Theta]", ",", "\[Phi]"}], "}"}], ",", 
        "0"}], "]"}]}], ",", 
     RowBox[{"hlmbarf", "\[Rule]", 
