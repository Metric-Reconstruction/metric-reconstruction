Cell[TextData[{
 "All Terms Together: ",
 Cell[BoxData[
  RowBox[{
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SubsuperscriptBox[
          StyleBox["G", "TI"], "\[Mu]\[Nu]", 
          RowBox[{"(", "1", ")"}]], 
         RowBox[{"[", 
           StyleBox["h", "TI"], "]"}], "\[LongEqual]", "-", 
         SuperscriptBox["\[Del]", "2"], 
         SubscriptBox[
          StyleBox["h", "TI"], "\[Mu]\[Nu]"], "+", 
         SuperscriptBox["\[Del]", "\[Rho]"], 
         SubscriptBox["\[Del]", "\[Mu]"], 
         SubscriptBox[
          StyleBox["h", "TI"], "\[Nu]\[Rho]"], "+", 
         SuperscriptBox["\[Del]", "\[Rho]"], 
         SubscriptBox["\[Del]", "\[Nu]"], 
         SubscriptBox[
          StyleBox["h", "TI"], "\[Mu]\[Rho]"], "-", 
         SubscriptBox["\[Del]", "\[Mu]"], 
         SubscriptBox["\[Del]", "\[Nu]"], 
         StyleBox["h", "TI"], "+", 
         SubscriptBox[
          StyleBox["g", "TI"], "\[Mu]\[Nu]"], 
         RowBox[{"(", 
           RowBox[{
             SuperscriptBox["\[Del]", "2"], 
             StyleBox["h", "TI"], "-", 
             SuperscriptBox["\[Del]", "\[Rho]"], 
             SuperscriptBox["\[Del]", "\[Sigma]"], 
             SubscriptBox[
              StyleBox["h", "TI"], "\[Rho]\[Sigma]"]}], ")"}]}], 
       TraditionalForm], "errors" -> {}, "input" -> 
     "G^{(1)}_{\\mu\\nu}[h]=-\\nabla^2 h_{\\mu \\nu}+\\nabla^\\rho \
\\nabla_\\mu h_{\\nu \\rho} + \\nabla^\\rho \\nabla_\\nu h_{\\mu \\rho} - \
\\nabla_{\\mu} \\nabla_\\nu h + g_{\\mu\\nu}(\\nabla^2 h - \\nabla^\\rho \
\\nabla^\\sigma h_{\\rho \\sigma})", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], " "}]], "Input",ExpressionUUID->
  "fe0a7ec4-77a9-4a3c-8761-d21cbd5d3691"]
}], "Text",
