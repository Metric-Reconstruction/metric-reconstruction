          RowBox[{"[", 
           RowBox[{"2", ",", "2"}], "]"}], "]"}], "/.", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"t", "->", "x1"}], ",", 
           RowBox[{"r", "->", "x2"}], ",", 
           RowBox[{"\[Theta]", "->", "x3"}], ",", 
